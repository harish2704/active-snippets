            "title": "Name of your model",
            "type": "string"
          },
          "sample": {
            "title": "Sample data(JSON)",
            "type": "string"
          }
        }
      },
      "template": "<%\nfunction capitalize( str ){\n    return str[0].toUpperCase() + str.slice(1);\n}\nvar modelName = capitalize( name );\nvar sampleData = JSON.parse( sample );\n%>\n\n\n\n/**\n * <%= modelName %>\n\n *\n * \n * @param {Object} args - Argument object\n <% Object.keys( sampleData ).forEach( function(key) { %>\n <% var item = sampleData[key]; %>\n * @param {<%= item && item.constructor.name %>} args.<%= key %> - <%= key %>\n <% }) %>\n * @return\n */\n \n function <%= modelName %>(args){\n<% for ( key in sampleData ) { %>\n    this.<%= key %> = args.<%= key %>;\n<% } %>\n}\n\n",
      "id": 7,
      "syntax": "javascript",
      "name": "Generic model"
